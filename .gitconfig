[user]
  name = Jingwen Owen Ou
  email = jingweno@gmail.com
[alias]
  st = status
  co = checkout
  pom = push origin master
  br = branch
  ci = commit
  rb = rebase
  df = diff
  lp = log -p
  l = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset'
  lol = log --pretty=oneline --abbrev-commit --graph --decorate
  unstage = reset HEAD
  staged = diff --cached
  unstaged = diff
  current-branch = !git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||'
  track = checkout -t
  who = shortlog -s --
[status]
  submodule = 1
[push]
  default = current
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
  interactive = auto
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  pager = less -FRSX
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = /opt/boxen/config/git/gitignore
  editor = vim
[apply]
  whitespace = warn
[heroku]
  account = jingweno
[branch]
  autosetuprebase = always
[mergetool]
  keepbackup = false
  keeptemporaries = false
  prompt = false
[help]
  autocorrect = 1

[credential]
helper = /opt/boxen/bin/boxen-git-credential

[hub]
protocol = git

[gh]
protocol = https
