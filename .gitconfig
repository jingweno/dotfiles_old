[user]
  name = Jingwen Owen Ou
  email = jingweno@gmail.com
[alias]
  st = status
  co = checkout
  cm = commit
  pom = push origin master
  br = branch
  ci = commit
  rb = rebase
  df = diff
  lg = log -p
  who = shortlog -s --
  oneline = log --pretty='%Cblue%h%Creset - %an - %Cgreen%s%Creset'
  hist = log --pretty=format:\"%h | %ad | %s %Cgreen[%an]%Creset%Cred%d%Creset\" --graph --date=short
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
  interactive = auto
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  pager = less -FRSX
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = /Users/Owen/.gitignore
  editor = vim
  autocrlf = input
[apply]
  whitespace = nowarn
[heroku]
  account = jingweno
[branch]
  autosetuprebase = always
